{
	"name": "client",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@apollo/client": "3.3.18",
		"@apollo/react-testing": "3.1.4",
		"@babel/core": "7.14.0",
		"@material-ui/core": "4.11.4",
		"@testing-library/jest-dom": "5.12.0",
		"@testing-library/react": "11.2.6",
		"@testing-library/user-event": "12.8.3",
		"@types/bootstrap": "5.0.12",
		"@types/enzyme": "3.10.8",
		"@types/enzyme-adapter-react-16": "1.0.6",
		"@types/enzyme-to-json": "1.5.4",
		"@types/hookrouter": "2.2.5",
		"@types/jest": "26.0.23",
		"@types/js-cookie": "2.2.6",
		"@types/lodash": "4.14.169",
		"@types/node": "12.20.10",
		"@types/react": "16.14.2",
		"@types/react-bootstrap": "0.32.24",
		"@types/react-bootstrap-table": "4.3.8",
		"@types/react-dom": "16.9.10",
		"@types/react-test-renderer": "17.0.1",
		"@types/styled-components": "5.1.9",
		"@typescript-eslint/parser": "4.22.0",
		"apollo": "2.32.13",
		"bootstrap": "4.6.0",
		"cross-fetch": "3.1.4",
		"enzyme": "3.11.0",
		"enzyme-adapter-react-16": "1.15.6",
		"enzyme-to-json": "3.6.2",
		"eslint": "7.25.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-import-resolver-typescript": "2.4.0",
		"eslint-plugin-import": "2.23.4",
		"eslint-plugin-prettier": "3.4.0",
		"eslint-plugin-sort-keys-fix": "1.1.1",
		"hookrouter": "1.2.3",
		"jest": "26.6.0",
		"jest-canvas-mock": "2.3.1",
		"jquery": "3.6.0",
		"js-cookie": "2.2.1",
		"lodash": "4.17.21",
		"material-table": "1.69.3",
		"popper.js": "1.16.1",
		"prettier": "2.2.1",
		"react": "16.14.0",
		"react-bootstrap": "1.4.0",
		"react-bootstrap-table": "4.3.1",
		"react-dom": "16.14.0",
		"react-scripts": "4.0.3",
		"react-test-renderer": "16.14.0",
		"styled-components": "5.2.3",
		"styled-reset": "4.3.4",
		"ts-jest": "26.5.6",
		"typesafe-actions": "4.4.2",
		"typescript": "4.2.4",
		"web-vitals": "1.1.1"
	},
	"scripts": {
		"build": "react-scripts build",
		"codegen": "npx apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --tagName=gql",
		"coverage": "jest --env=jsdom --coverage || open coverage/lcov-report/index.html",
		"eject": "react-scripts eject",
		"eslint": "eslint . --ext=.ts,.tsx",
		"eslint:fix": "eslint . --ext=.ts,.tsx --fix",
		"lintfix": "npx prettier && npm run eslint:fix",
		"prettier": "prettier --config .prettierrc.json --write 'src/**/*.{ts,tsx}'",
		"schema:update": "npx apollo service:download --endpoint=http://localhost:4000/ graphql-schema.json",
		"start": "react-scripts start",
		"test": "jest --env=jsdom"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"rules": {
			"@typescript-eslint/no-empty-function": 0,
			"react/display-name": 0,
			"react/prop-types": 0
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
